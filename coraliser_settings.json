{
  "mcpServers": {
    "yona": {
      "command": "python",
      "args": ["yona_simple_mcp_server.py"],
      "env": {
        "OPENAI_API_KEY": "OPENAI_API_KEY",
        "SUPABASE_URL": "SUPABASE_URL", 
        "SUPABASE_KEY": "SUPABASE_KEY"
      }
    }
  },
  "coral_server_url": "http://coral.pushcollective.club:5555",
  "settings": {
    "log_level": "INFO",
    "timeout": 30,
    "retry_attempts": 3,
    "enable_cors": true
  }
}
